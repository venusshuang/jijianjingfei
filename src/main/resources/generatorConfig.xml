<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<classPathEntry location="D:/mysql-connector-java-5.1.7-bin.jar" />
	<context id="context1">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/jijianjingfei?characterEncoding=utf8"
			userId="root" password="123123" />
		<javaModelGenerator targetPackage="jijianjingfei.model"
			targetProject="jijianjingfei" />
		<sqlMapGenerator targetPackage="jijianjingfei.mapper"
			targetProject="jijianjingfei" />
		<javaClientGenerator targetPackage="jijianjingfei.dao"
			targetProject="jijianjingfei" type="XMLMAPPER" />

		<table schema="jijianjingfei" tableName="user" />
		
	</context>
</generatorConfiguration>