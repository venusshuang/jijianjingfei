<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<classPathEntry location="D:/mysql-connector-java-5.1.7-bin.jar" />
	<context id="context1" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/jjjf?serverTimezone=UTC&amp;characterEncoding=UTF-8"
			userId="root" password="123123" />
		<javaModelGenerator targetPackage="jjjf.model"
			targetProject=".\src\main\java" />
		<sqlMapGenerator targetPackage="jjjf.mapper"
			targetProject=".\src\main\java" />
		<javaClientGenerator targetPackage="jjjf.dao"
			targetProject=".\src\main\java" type="XMLMAPPER" />

		<table schema="jjjf" tableName="user1" />
		
	</context>
</generatorConfiguration>